{
  "name": "mongodb-runner",
  "version": "2.2.0",
  "author": "Lucas Hrabovsky <hrabovsky.lucas@gmail.com> (http://imlucas.com)",
  "description": "Starts all of the deployment types for testing",
  "scripts": {
    "check": "mongodb-js-precommit",
    "test": "mocha",
    "ci": "./node_modules/istanbul/lib/cli.js cover _mocha -- -R spec ./test/*.test.js"
  },
  "bin": {
    "mongodb-runner": "./bin/mongodb-runner.js"
  },
  "homepage": "http://github.com/mongodb-js/runner",
  "repository": {
    "type": "git",
    "url": "git://github.com/mongodb-js/runner.git"
  },
  "check": {
    "entries": [
      "bin/*.js"
    ],
    "ignore": [
      "coverage/{*,**/*}"
    ]
  },
  "precommit": [
    "check",
    "test"
  ],
  "keywords": [
    "mongodb",
    "mongodb.js"
  ],
  "license": "Apache-2.0",
  "dependencies": {
    "debug": "~2.2.0",
    "lodash.defaults": "^3.1.2",
    "minimist": "^1.1.2",
    "mkdirp": "^0.5.1",
    "mongodb-dbpath": "0.0.1",
    "mongodb-tools": "^1.0.5",
    "untildify": "~2.1.0"
  },
  "devDependencies": {
    "coveralls": "^2.11.2",
    "eslint-config-mongodb-js": "^0.1.5",
    "github-release": "~0.2.0",
    "gulp": "~3.9.0",
    "istanbul": "^0.3.17",
    "kill-mongodb": "0.0.1",
    "lone": "~0.4.2",
    "mocha": "~2.2.5",
    "pre-commit": "^1.0.10"
  }
}
